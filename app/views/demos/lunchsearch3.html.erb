<%= render "partial/navbar" %>

<mains class="" id="main-collapse">

<%= form_tag(demos_lunch_search2_path, method: :get) do %>
	<%= hidden_field_tag :city_lunch, params[:city_lunch] %>
	<%= hidden_field_tag :price_lunch, params[:price_lunch] %>
	<%= hidden_field_tag :small_lunch, params[:small_lunch] %>
	<%= hidden_field_tag :spot1, params[:spot1] %>
	<%= hidden_field_tag :distance_second, 1 %>

		<div class="box95" id=''>
		  	<div>
		  		<h3 class="box-top" style="color:white;text-shadow: 1px 1px 2px black;">この後はどうしますか？</h3>
		  	</div>
		  	<div class="img_radio-home">
			 	<%= radio_button_tag(:large_second, "おまかせ", true, {:id => "adate"}) %>
					<%= label_tag('adate', "A.dateにおまかせ") %> 
		  	</div>
		  	<div class="horizontal_scroll_search">
			  <ul class="scroll_lst_search">
			  	<li class="scroll_item_search">
			        <span class="img_radio0">	
					 	<%= radio_button_tag(:large_second, "２軒目なし", false, {:id => "not"}) %>
							<%= label_tag('not', "２軒目なし") %> 
					</span>
			    </li>
			    <li class="scroll_item_search">
			        <span class="img_radio-cafe">	
					 	<%= radio_button_tag(:large_second, "昼カフェ", false, {:id => "cafe"}) %>
							<%= label_tag('cafe', "カフェ") %>
					</span>
			    </li>
			    <li class="scroll_item_search">
			        <span class="img_radio-park">	
					 	<%= radio_button_tag(:large_second, "公園", false, {:id => "park"}) %>
							<%= label_tag('park', "公園") %> 
					</span>
			    </li>
			    <li class="scroll_item_search">
			        <span class="img_radio-museum">	
					 	<%= radio_button_tag(:large_second, "ミュージアム", false, {:id => "museum"}) %>
							<%= label_tag('museum', "ミュージアム") %> 
					</span>
			    </li>
			    <li class="scroll_item_search">
			        <span class="img_radio-shop">	
					 	<%= radio_button_tag(:large_second, "ショップ", false, {:id => "shop"}) %>
							<%= label_tag('shop', "ショップ") %> 
					</span>
			    </li>
			    <li class="scroll_item_search">
			        <span class="img_radio-walk">	
					 	<%= radio_button_tag(:large_second, "食べ歩き", false, {:id => "walk"}) %>
							<%= label_tag('walk', "食べ歩き") %> 
					</span>
			    </li>
			    <li class="scroll_item_search">
			        <span class="img_radio-active">	
					 	<%= radio_button_tag(:large_second, "昼アクティブ", false, {:id => "active"}) %>
							<%= label_tag('active', "アクティブ") %> 
					</span>
			    </li>
			    <li class="scroll_item_search">
			        <span class="img_radio-recommend">	
					 	<%= radio_button_tag(:large_second, "その他", false, {:id => "other"}) %>
							<%= label_tag('other', "その他") %>
					</span>
			    </li>
			  </ul>
			</div>
			<br>
			<h1><%= submit_tag "次へ進む", :class => "btn btn-default" %></h1>
		  </div>

		
<% end %>



</mains>

<script>
document.addEventListener("DOMContentLoaded", function (event) {
  navbarToggleSidebar();
  navActivePage();
});
</script>

