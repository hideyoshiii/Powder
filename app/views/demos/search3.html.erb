<%= render "partial/navbar" %>

<mains class="" id="main-collapse">

<%= form_tag(demos_search2_path, method: :get) do %>
	<%= hidden_field_tag :city_dinner, params[:city_dinner] %>
	<%= hidden_field_tag :price_dinner, params[:price_dinner] %>
	<%= hidden_field_tag :small_dinner, params[:small_dinner] %>
	<%= hidden_field_tag :distance, 1 %>
	<%= hidden_field_tag :spot1, params[:spot1] %>

		<div class="box5" id='section4'>
	  		<h3 style="color:white;text-shadow: 1px 1px 2px black;">
	  			<div class="text-center">
	  				この後はどうしますか？
	  			</div>
	  		</h3>
	  		<div class="row radio-box">
	  			<div class="col-xs-4 col-sm-4 col-md-4">
	  				<div class="text-center">
	  					<span class="img_radio1">	
		 					<%= radio_button_tag(:large, "バー", true, {:id => "bar"}) %>
							<%= label_tag('bar', "バー") %>
						</span>
					</div>
				</div>
				<div class="col-xs-4 col-sm-4 col-md-4">
					<div class="text-center">
						<span class="img_radio2">	
				  			<%= radio_button_tag(:large, "夜カフェ", false, {:id => "cafe"}) %>
							<%= label_tag('cafe', "夜カフェ") %> 
						</span>
					</div>
				</div>
				<div class="col-xs-4 col-sm-4 col-md-4">
					<div class="text-center">
						<span class="img_radio0">	
							<%= radio_button_tag(:large, "２軒目なし", false, {:id => "not"}) %>
							<%= label_tag('not', "２軒目なし") %> 
						</span>
					</div>
				</div>
			</div>
	  		<div>
				<h1><%= submit_tag "次へ進む", :class => "btn btn-default" %></h1>
			</div>
	  	</div>
	   
<% end %>



</mains>

<script>
document.addEventListener("DOMContentLoaded", function (event) {
  navbarToggleSidebar();
  navActivePage();
});
</script>

