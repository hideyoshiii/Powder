<%= render "partial/navbar" %>
<mains class="" id="main-collapse">

<%= form_tag(searches_category_first_path, method: :get) do %>
  <%= hidden_field_tag :timezone, params[:timezone] %>

  <div class="box93" id=''>
  	<div>
  		<h4 class="box-top1" style="color:white;text-shadow: 1px 1px 2px black;">ではデートコースを作っていきましょう。</h4>
  	</div>
  	<br>
  	<div>
  		<h3 class="box-top" style="color:white;text-shadow: 1px 1px 2px black;">どんなデートにしたいですか？</h3>
  	</div>
  	<div class="horizontal_scroll_search">
	  <ul class="scroll_lst_search">
	    <li class="scroll_item_search">
	        <span class="img_radio-casual">	
			 	<%= radio_button_tag(:scene, "カジュアル", false, {:id => "casual"}) %>
					<%= label_tag('casual', "カジュアル") %>
			</span>
	    </li>
	    <li class="scroll_item_search">
	        <span class="img_radio-cool">	
			 	<%= radio_button_tag(:scene, "クール", false, {:id => "cool"}) %>
					<%= label_tag('cool', "クール") %>
			</span>
	    </li>
	    <li class="scroll_item_search">
	        <span class="img_radio-unique">	
			 	<%= radio_button_tag(:scene, "ユニーク", false, {:id => "unique"}) %>
					<%= label_tag('unique', "ユニーク") %>
			</span>
	    </li>
	  </ul>
	</div>
	<h1><%= submit_tag "次へ進む", :class => "btn btn-default" %></h1>
  </div>


<% end %>


<script>
//更新
function scroll(){
	var n = window.location.href.slice(window.location.href.indexOf('?') + 4);
    var p = $("section1").offset().top;
    $('html,body').animate({ scrollTop: p }, 'slow');
    return false;
}
</script>


</mains>

<script>
document.addEventListener("DOMContentLoaded", function (event) {
  navbarToggleSidebar();
  navActivePage();
});
</script>

