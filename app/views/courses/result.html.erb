	              <span class="insta"><i class="fa fa-phone icon"></i></span>
	            </a>
	          <% end %>
	        </div>
	      <%# GoogleMapボタン %>
	        <% if @spot1.address %>
	          <a href="javascript:;" onclick="window.open('http://maps.google.co.jp/maps?q='+encodeURI('<%= @spot1.address %>'));return false;" href="javascript:;" class="fl_inst pull-left">
	            <span class="insta"><i class="fa fa-map-marker icon"></i></span>
	          </a>
	        <% end %>
	      <%# Rettyボタン %>
	        <% if @spot1.retty %>
	          <a href="<%= @spot1.retty %>" target="_blank" class="fl_inst pull-left">
	            <span class="insta"><i class="fa fa-cutlery icon"></i></span>
	          </a>
	        <% end %>
	      <%# Instagramボタン %>
