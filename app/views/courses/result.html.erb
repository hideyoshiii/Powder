          <p class="aaa">Description</p>
          <p><%= @spot1.description %></p>
          <% end %>
          <%# 諸情報、リンク等 %>
          <hr class="more">
          <div style="color:black;">More</div>
          <%# 公式サイトボタン %>
	        <% if @spot1.browser %>
	          <a href="<%= @spot1.browser %>" target="_blank" class="fl_inst pull-left">
	            <span class="insta"><i class="fa fa-safari icon"></i></span>
	          </a>
	        <% end %>
	      <%# 電話ボタン %>
	        <div class="visible-xs">
