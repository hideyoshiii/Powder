	<% if (user_signed_in?) %>
      <a href="" data-toggle="modal" data-target="#myModal_station">
        <div class="text-center" style="margin-top:-5px;">
          <i class="fas fa-angle-down"></i>&nbsp;駅から決める
        </div>
      </a>
    <% end %>
    
    <br>

    <!-- モーダルウィンドウの中身 -->
    <div class="modal" id="myModal_station">
      <div class="modal-dialog">
        <div style="height:5vh;"></div>
        <div class="modal-content" style="max-height:70vh;">
          <div class="box-modal text-center" style="padding:80px 0px 90px;">

            <p style="color:white;">ご指定の駅周辺で最適なコースを提案します</p>
            <p style="color:white;">駅名を入力してください</p>
            <div style="height:30px;"></div>

            <%= form_tag(plans_timezone_path, method: :get) do %>
              <div class="text-box01">
                <div class="form-group" style="width:50%;display:inline-block;">
                  <%= text_field_tag :station, "", placeholder: "恵比寿", :required => true, class: "form-control form-control2" %>
                </div>
                <div style="color:white;display:inline-block;">
                  駅
                </div>
              </div>

          </div>
        </div>
        <div class="btn-modal">
            <div class="btn-modal-next">
              <%= button_tag :type => "submit", :class =>"button-half" do %>
                <div>
                  駅を指定して次へ進む
                </div>
              <% end %>
            </div>
          <% end %>
          <a href="" data-toggle="modal" data-dismiss="modal">
            <div class="btn-modal-back">
              <div>
                閉じる
              </div>
            </div>
          </a>  
        </div>
      </div>
    </div>






































