<header class="">
  <div class="navbar navbar-default visible-xs">
    <button type="button" class="navbar-toggle collapsed">
      <span class="sr-only">Toggle navigation</span>
      <span class="icon-bar"></span>
      <span class="icon-bar"></span>
      <span class="icon-bar"></span>
    </button>
    <a href="/" class="navbar-brand">Powder</a>
  </div>

  <nav class="sidebar">
    <div class="navbar-collapse" id="navbar-collapse">
      <div class="site-header hidden-xs">
          <a class="site-brand" href="/" title="">
            <h2>
              <%= image_tag 'mashup-logo.svg', :class =>'site-logo' %>
              Powder
            </h2>
          </a>
        <p>あなたのデートコース</p>
      </div>
      <ul class="nav">
        <li>Book</li>
        


      </ul>

      <nav class="nav-footer">
        <% if (user_signed_in?) %>
        <%= link_to user_path(current_user) do %>
        
        <%= current_user.name %>
        <% end %>
        <br>
        <%= link_to "ログアウト", destroy_user_session_path, method: :delete %>
        <% else %>
        <%= link_to "Sign in with Facebook", user_facebook_omniauth_authorize_path %>
        <% end %>
        <p class="nav-footer-social-buttons">
          <a class="fa-icon" href="https://www.instagram.com/myi.no1/" target="_blank">
            <i class="fa fa-instagram"></i>
          </a>
          <a class="fa-icon" href="https://www.facebook.com/myi.no1/" target="_blank">
            <i class="fa fa-facebook" aria-hidden="true"></i>
          </a>
          <a class="fa-icon" href="https://twitter.com/myi_no1/" target="_blank">
            <i class="fa fa-twitter"></i>
          </a>
        </p>
        <p>© You | Website created with <a href="http://www.mashup-template.com/" title="Create website with free html template">Mashup Template</a>/<a href="https://www.unsplash.com/" title="Beautiful Free Images">Unsplash</a></p>
      </nav>  
    </div> 
  </nav>
</header>
<main class="" id="main-collapse">

	<div class="container">
	<div class="row">
		<div class="col-md-6 col-md-offset-3">
			<div class="panel panel-default">
				<div class="panel-heading">
					<div class="text-center">
						<span style="font-size:24px;">Question</span>
					</div>
				</div>

	<div class="panel-body">			
					<!-- ここからフォーム -->
					<%= form_for @question do |f| %>


						<br>

						<div class="row">
							<div class="col-md-12 select">
								<div class="form-group">
									<label>scene</label>
									<%= f.select :category, [["誘う","誘う"], ["プラン","プラン"], ["当日","当日"], ["デート後","デート後"], ["メッセージ","メッセージ"], ["その他","その他"]], {prompt:"場面を選択してください"}, {required: 'true', autofocus: 'true', class: 'form-control'} %>
								</div>
							</div>
						</div>
						<br>
					

						<div class="row">
							<div class="col-md-12 select">
								<div class="form-group">
                                <label>content</label>
                                <%= f.text_area :content, rows: 4, placeholder: "質問を記入してください", class: "form-control", required: "true" %>
                            </div> 
							</div>
						</div>
						<br>

						<div class="row">
			            <div class="col-md-12 select">
			              <div class="form-group">
			                <label>photo&nbsp;※もしあれば</label>
			            <div class="field">
			              <%= f.file_field :photo %>
			            </div>
			            </div>
			            </div>
			            </div>
			            <br>

			            <%= f.hidden_field :sex, value: current_user.sex %>
					

						<div class="actions">
							<%= f.submit "Save", class: "btn btn-danger" %>
						</div>
					<% end %>
				</div>
			</div>
		</div>
	</div>

</div>


 
</main>

<script>
document.addEventListener("DOMContentLoaded", function (event) {
  navbarToggleSidebar();
  navActivePage();
});
</script>

