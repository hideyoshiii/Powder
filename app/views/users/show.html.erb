<%= render "partial/navbar4" %>
<main class="" id="main-collapse">
<br>

<% if @user == current_user %>
  <%= link_to edit_user_registration_path(current_user) do %>
    <div class="img-show black">
      <%= image_tag @user.image, :class => "img-circle profile-1" %>
    <%= @user.nickname %>
    </div>
  <% end %>
<% else %>
