<header class="">
  <div class="navbar navbar-default visible-xs">
    <button type="button" class="navbar-toggle collapsed">
      <span class="sr-only">Toggle navigation</span>
      <span class="icon-bar"></span>
      <span class="icon-bar"></span>
      <span class="icon-bar"></span>
    </button>
    <a href="/" class="navbar-brand">Powder</a>
  </div>

  <nav class="sidebar">
    <div class="navbar-collapse" id="navbar-collapse">
      <div class="site-header hidden-xs">
          <a class="site-brand" href="/" title="">
            <h2>
              <%= image_tag 'mashup-logo.svg', :class =>'site-logo' %>
              Powder
            </h2>
          </a>
        <p>あなたのデートコース</p>
      </div>
      <ul class="nav">
        <li>Book</li>
        


      </ul>

      <nav class="nav-footer">
        <% if (user_signed_in?) %>
        <%= link_to user_path(current_user) do %>
        <%= image_tag current_user.image, :class => "img-circle profile-0" %>
        &nbsp;&nbsp;<%= current_user.nickname %>
        <% end %>
        <br>
        <%= link_to "ログアウト", destroy_user_session_path, method: :delete %>
        <% else %>
        <%= link_to "Sign in with Facebook", user_facebook_omniauth_authorize_path %>
        <% end %>
        <p class="nav-footer-social-buttons">
          <a class="fa-icon" href="https://www.instagram.com/myi.no1/" target="_blank">
            <i class="fa fa-instagram"></i>
          </a>
          <a class="fa-icon" href="https://www.facebook.com/myi.no1/" target="_blank">
            <i class="fa fa-facebook" aria-hidden="true"></i>
          </a>
          <a class="fa-icon" href="https://twitter.com/myi_no1/" target="_blank">
            <i class="fa fa-twitter"></i>
          </a>
        </p>
        <p>© You | Website created with <a href="http://www.mashup-template.com/" title="Create website with free html template">Mashup Template</a>/<a href="https://www.unsplash.com/" title="Beautiful Free Images">Unsplash</a></p>
      </nav>  
    </div> 
  </nav>
</header>
<main class="" id="main-collapse">

<div class="img-show">
  <%= image_tag @user.image, :class => "img-circle profile-1" %>
  <br>
  <%= @user.nickname %>
  <br>
  <div class="like">
  <% want_count = Like.where(user_id: @user.id, kind: 1).count %>
        <i class="fa fa-gratipay want-btn" aria-hidden="true"></i>&nbsp;<%= want_count %>
        &nbsp;&nbsp;&nbsp;
        <% done_count = Like.where(user_id: @user.id, kind: 2).count %>
        <i class="fa fa-check-circle done-btn" aria-hidden="true"></i>&nbsp;<%= done_count %>
        </div>
        <br>
</div>
<div class="row">
<div class="col-xs-12 col-sm-12 col-md-offset-2 col-md-8">
<div class="panel panel-default">
  <%= link_to "/users/#{@user.id}/want" do %>
<div class="panel-heading like-font-user-show">
  want
  <div class="pull-right">
    <i class="fa fa-chevron-right" aria-hidden="true"></i>
  </div>
  <% end %>
</div>
<% @wants.each do |want| %>
<% spot = Spot.find_by(id: want.spot_id) %>
<%= link_to spot_path(spot) do %>
<%= image_tag spot.photo.url, :class =>'like-img-user-show' %>
<% end %>
<% end %>
</div>
</div>
</div>

<br>

<div class="row">
<div class="col-xs-12 col-sm-12 col-md-offset-2 col-md-8">
<div class="panel panel-default">
<div class="panel-heading like-font-user-show">
  <%= link_to "/users/#{@user.id}/done" do %>
  done
  <div class="pull-right">
    <i class="fa fa-chevron-right" aria-hidden="true"></i>
  </div>
  <% end %>
</div>
<% @dones.each do |done| %>
<% spot = Spot.find_by(id: done.spot_id) %>
<%= link_to spot_path(spot) do %>
<%= image_tag spot.photo.url, :class =>'like-img-user-show' %>
<% end %>
<% end %>
</div>
</div>
</div>
 
</main>

<script>
document.addEventListener("DOMContentLoaded", function (event) {
  navbarToggleSidebar();
  navActivePage();
});
</script>

