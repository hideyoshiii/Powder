<%= render "partial/navbar3" %>
<main class="" id="main-collapse">

	<div class="row">
		<div class="col-xs-12 col-sm-offset-2 col-sm-8 col-sm-offset-2 col-md-8">

			<div class="row">
				<div class="col-xs-12 col-sm-12 col-md-12">
					<div class="article-title-show">
						<%= @article.title %>
					</div>
					<div class="article-city-show">
						<i class="fa fa-map-marker" aria-hidden="true"></i> <%= @article.prefecture %> > <%= @article.city %>
					</div>	
				</div>
			</div>



			<div class="row">	
				<hr>
				<blockquote class="instagram-media" data-instgrm-version="7" style=" background:#FFF; border:0; border-radius:3px; box-shadow:0 0 1px 0 rgba(0,0,0,0.5),0 1px 10px 0 rgba(0,0,0,0.15); margin: 1px; max-width:99.375%; padding:0; width:99.375%; width:-webkit-calc(100% - 2px); width:calc(100% - 2px);"><div style="padding:8px;"> <div style=" background:#F8F8F8; line-height:0; margin-top:40px; padding:28.125% 0; text-align:center; width:100%;"> <div style=" background:url(data:image/png;base64,); display:block; height:44px; margin:0 auto -44px; position:relative; top:-22px; width:44px;"></div></div><p style=" color:#c9c8cd; font-family:Arial,sans-serif; font-size:14px; line-height:17px; margin-bottom:0; margin-top:8px; overflow:hidden; padding:8px 0 7px; text-align:center; text-overflow:ellipsis; white-space:nowrap;"><a href="https://www.instagram.com/p/BcUWstXDhyg/" style=" color:#c9c8cd; font-family:Arial,sans-serif; font-size:14px; font-style:normal; font-weight:normal; line-height:17px; text-decoration:none;" target="_blank">Powder パウダーさん(@powder.date)がシェアした投稿</a> - <time style=" font-family:Arial,sans-serif; font-size:14px; line-height:17px;" datetime="2017-12-05T11:04:43+00:00">2017 12月 5 3:04午前 PST</time></p></div></blockquote> <script async defer src="//platform.instagram.com/en_US/embeds.js"></script>		
				<br>

				<div class="row">
					<div class="col-xs-12 col-sm-12 col-md-12">
						<div class="img-show">

						<a href="http://www.facebook.com/sharer.php?u=<%= url_for(only_path: false) %>&t=Powder/あなたのデートに彩りを。" target="_blank" class="facebook white">Facebook</a>

						<a href="https://twitter.com/share?ref_src=twsrc%5Etfw&text=Powder/あなたのデートに彩りを。" target="_blank" class="twitter white" data-show-count="false">Tweet</a><script async src="https://platform.twitter.com/widgets.js" charset="utf-8"></script>

						<a href="http://line.me/R/msg/text/? Powder/あなたのデートに彩りを。
						<%= url_for(only_path: false) %>" target="_blank" class="line white"> LINE</a>

						</div>
					</div>
				</div>

				
				<hr>
				<div class="row">
					<div class="col-xs-12 col-sm-12 col-md-12">
						<div style="font-size:20px;">この動画に含まれるスポット</div>
						</div>	
					</div>
				<br>
				<% a = Spot.find(1) %>
				<div class="row">
					<div class="col-xs-6 col-sm-6 col-md-4">
						<%= link_to spot_path(a) do %>
							<div class="img-show">
								<%= image_tag a.photo.url, :class =>'img-article-index' %>
							</div>
							<h4 class="black img-show title-font-index"><%= a.title %></h4>
						<% end %>
						<div class="img-show prefecture-font-index">
				          <span style="color: #CCCCCC;"><i class="fa fa-map-marker" aria-hidden="true"></i><%= a.prefecture %></span>
				        </div>
					</div>	
				</div>

			</div>
		</div>
	
	</div>
 
</main>

<script>
document.addEventListener("DOMContentLoaded", function (event) {
  navbarToggleSidebar();
  navActivePage();
});
</script>

