<%= render "partial/navbar" %>

<main class="" id="main-collapse">

	<div class="row">

<div class="col-xs-12 col-sm-offset-1 col-sm-10 col-md-offset-2 col-md-8">

<div class="panel panel-default">
	
	<%= search_form_for @q, url: search_osaka1_path do |f| %>
    <div class="panel-body">
      	<div class="form-group">
      		<div class="city-check">
	        <label class="checkbox-inline">
			 <%= f.check_box :city_eq_any, { multiple: true },checked_value = "京橋・OBP", unchecked_value = "" %>京橋・OBP　
			</label>
			<label class="checkbox-inline">
			  <%= f.check_box :city_eq_any, { multiple: true },checked_value = "新大阪・西中島・十三", unchecked_value = "" %>新大阪・西中島・十三　
			</label>
			<label class="checkbox-inline">
			  <%= f.check_box :city_eq_any, { multiple: true },checked_value = "西天満・南森・天満橋", unchecked_value = "" %>西天満・南森・天満橋　
			</label>
			<label class="checkbox-inline">
			  <%= f.check_box :city_eq_any, { multiple: true },checked_value = "泉北", unchecked_value = "" %>泉北　
			</label>
			<label class="checkbox-inline">
			 <%= f.check_box :city_eq_any, { multiple: true },checked_value = "大阪キタ", unchecked_value = "" %>大阪キタ　
			</label>
			<label class="checkbox-inline">
			  <%= f.check_box :city_eq_any, { multiple: true },checked_value = "大阪ベイエリア", unchecked_value = "" %>大阪ベイエリア　
			</label>
			<label class="checkbox-inline">
			  <%= f.check_box :city_eq_any, { multiple: true },checked_value = "大阪ミナミ", unchecked_value = "" %>大阪ミナミ　
			</label>
			<label class="checkbox-inline">
			  <%= f.check_box :city_eq_any, { multiple: true },checked_value = "谷町・上本町・鶴橋", unchecked_value = "" %>谷町・上本町・鶴橋　
			</label>
			<label class="checkbox-inline">
			 <%= f.check_box :city_eq_any, { multiple: true },checked_value = "淀屋橋・本町・北浜", unchecked_value = "" %>淀屋橋・本町・北浜　
			</label>
			<label class="checkbox-inline">
			  <%= f.check_box :city_eq_any, { multiple: true },checked_value = "北大阪・千里中央", unchecked_value = "" %>北大阪・千里中央　
			</label>
			<label class="checkbox-inline">
			  <%= f.check_box :city_eq_any, { multiple: true },checked_value = "北河内・東大阪", unchecked_value = "" %>北河内・東大阪　
			</label>
			
	        </div>	    
      	
    
        <div class="pull-right">
        <%= f.submit "検索", class: "btn btn-default" %>
        </div>
        </div>
    </div>

      
    <br>  
<% end %>
</div>
</div>
</div>
 
</main>

<script>
document.addEventListener("DOMContentLoaded", function (event) {
  navbarToggleSidebar();
  navActivePage();
});
</script>

