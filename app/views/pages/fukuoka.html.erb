<%= render "partial/navbar" %>

<main class="" id="main-collapse">

	<div class="row">

<div class="col-xs-12 col-sm-offset-1 col-sm-10 col-md-offset-2 col-md-8">

<div class="panel panel-default">
	
	<%= search_form_for @q, url: search_fukuoka1_path do |f| %>
    <div class="panel-body">
    	<div class="form-title">
              エリアを絞る
        </div> 
        <hr class="style1">
      	<div class="form-group">
      		<div class="city-check">
	        <label class="checkbox-inline">
			 <%= f.check_box :city_eq_any, { multiple: true },checked_value = "久留米・大牟田・筑後", unchecked_value = "" %>久留米・大牟田・筑後　
			</label>
			<label class="checkbox-inline">
			  <%= f.check_box :city_eq_any, { multiple: true },checked_value = "糸島・姪浜・福岡市西区", unchecked_value = "" %>糸島・姪浜・福岡市西区　
			</label>
			<label class="checkbox-inline">
			  <%= f.check_box :city_eq_any, { multiple: true },checked_value = "住吉・呉服・その他博多区", unchecked_value = "" %>住吉・呉服・その他博多区　
			</label>
			<label class="checkbox-inline">
			  <%= f.check_box :city_eq_any, { multiple: true },checked_value = "西新・百道・その他早良区", unchecked_value = "" %>西新・百道・その他早良区　
			</label>
			<label class="checkbox-inline">
			 <%= f.check_box :city_eq_any, { multiple: true },checked_value = "太宰府・春日・筑紫野", unchecked_value = "" %>太宰府・春日・筑紫野　
			</label>
			<label class="checkbox-inline">
			  <%= f.check_box :city_eq_any, { multiple: true },checked_value = "中洲・香椎・その他東区", unchecked_value = "" %>中洲・香椎・その他東区　
			</label>
			<label class="checkbox-inline">
			  <%= f.check_box :city_eq_any, { multiple: true },checked_value = "飯塚・宗像・筑豊", unchecked_value = "" %>飯塚・宗像・筑豊　
			</label>
			<label class="checkbox-inline">
			  <%= f.check_box :city_eq_any, { multiple: true },checked_value = "北九州", unchecked_value = "" %>北九州　
			</label>
			<label class="checkbox-inline">
			  <%= f.check_box :city_eq_any, { multiple: true },checked_value = "薬院・今泉・平尾", unchecked_value = "" %>薬院・今泉・平尾　
			</label>
			<label class="checkbox-inline">
			  <%= f.check_box :city_eq_any, { multiple: true },checked_value = "六本松・桜坂・大濠", unchecked_value = "" %>六本松・桜坂・大濠　
			</label>
			
	        </div>	    
      	
    
        <div class="pull-right">
        <%= f.submit "検索", class: "btn btn-default" %>
        </div>
        </div>
    </div>

      
    <br>  
<% end %>
</div>
</div>
</div>
 
</main>

<script>
document.addEventListener("DOMContentLoaded", function (event) {
  navbarToggleSidebar();
  navActivePage();
});
</script>

