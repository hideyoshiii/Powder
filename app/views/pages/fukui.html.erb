<%= render "partial/navbar" %>

<main class="" id="main-collapse">

<div class="row">

<div class="col-xs-12 col-sm-offset-1 col-sm-10 col-md-offset-2 col-md-8">
	<div class="panel panel-default">
		<%= search_form_for @q, url: search_fukui1_path do |f| %>
	    <div class="panel-body">
	      	<div class="form-title">
              エリアを絞る
        </div> 
        <hr class="style1">
	      		<div class="city-check">
		        <label class="checkbox-inline">
				 <%= f.check_box :station_eq_any, { multiple: true },checked_value = "坂井・あわら市", unchecked_value = "" %>坂井・あわら市　
				</label>
				<label class="checkbox-inline">
				  <%= f.check_box :station_eq_any, { multiple: true },checked_value = "鯖江・武生・越前海岸", unchecked_value = "" %>鯖江・武生・越前海岸　
				</label>
				<label class="checkbox-inline">
				  <%= f.check_box :station_eq_any, { multiple: true },checked_value = "勝山・大野・和泉", unchecked_value = "" %>勝山・大野・和泉　
				</label>
				<label class="checkbox-inline">
				  <%= f.check_box :station_eq_any, { multiple: true },checked_value = "小浜・高浜・あおい町", unchecked_value = "" %>小浜・高浜・あおい町　
				</label>
				<label class="checkbox-inline">
				  <%= f.check_box :station_eq_any, { multiple: true },checked_value = "敦賀・美浜・若狭", unchecked_value = "" %>敦賀・美浜・若狭　
				</label>
				<label class="checkbox-inline">
				  <%= f.check_box :station_eq_any, { multiple: true },checked_value = "福井・永平寺・美山", unchecked_value = "" %>福井・永平寺・美山　
				</label>
				

		        </div>	    
	     
		        <div class="pull-right">
		        <%= f.submit "検索", class: "btn btn-default btn-sm" %>
		        </div>
	        
	    </div> 
		<% end %>
	</div>
</div>

</div>
 
</main>

<script>
document.addEventListener("DOMContentLoaded", function (event) {
  navbarToggleSidebar();
  navActivePage();
});
</script>

