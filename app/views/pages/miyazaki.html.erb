<%= render "partial/navbar" %>

<main class="" id="main-collapse">

<div class="row">

<div class="col-xs-12 col-sm-offset-1 col-sm-10 col-md-offset-2 col-md-8">
	<div class="panel panel-default">
		<%= search_form_for @q, url: search_miyazaki1_path do |f| %>
	    <div class="panel-body">
	      	<div class="form-title">
              エリアを絞る
        </div> 
        <hr class="style1">
	      		<div class="city-check">
		        <label class="checkbox-inline">
				 <%= f.check_box :station_eq_any, { multiple: true },checked_value = "延岡・日向・臼杵郡", unchecked_value = "" %>延岡・日向・臼杵郡　
				</label>
				<label class="checkbox-inline">
				  <%= f.check_box :station_eq_any, { multiple: true },checked_value = "宮崎・東諸県部", unchecked_value = "" %>宮崎・東諸県部　
				</label>
				<label class="checkbox-inline">
				  <%= f.check_box :station_eq_any, { multiple: true },checked_value = "西部・児湯郡", unchecked_value = "" %>西部・児湯郡　
				</label>
				<label class="checkbox-inline">
				  <%= f.check_box :station_eq_any, { multiple: true },checked_value = "都城・小林", unchecked_value = "" %>都城・小林　
				</label>
				<label class="checkbox-inline">
				  <%= f.check_box :station_eq_any, { multiple: true },checked_value = "日南・串間", unchecked_value = "" %>日南・串間　
				</label>
				
		        </div>	    
	     
		        <div class="pull-right">
		        <%= f.submit "検索", class: "btn btn-default btn-sm" %>
		        </div>
	        
	    </div> 
		<% end %>
	</div>
</div>

</div>
 
</main>

<script>
document.addEventListener("DOMContentLoaded", function (event) {
  navbarToggleSidebar();
  navActivePage();
});
</script>

