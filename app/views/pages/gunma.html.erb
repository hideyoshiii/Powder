<%= render "partial/navbar" %>

<main class="" id="main-collapse">

<div class="panel panel-default">
	
	<%= search_form_for @q, url: search_gunma1_path do |f| %>
    <div class="panel-body">
      	<div class="form-group">
      		<div class="city-check">
	        <label class="checkbox-inline">
			 <%= f.check_box :city_eq_any, { multiple: true },checked_value = "伊勢崎・太田・館林・桐生", unchecked_value = "" %>伊勢崎・太田・館林・桐生　
			</label>
			<label class="checkbox-inline">
			  <%= f.check_box :city_eq_any, { multiple: true },checked_value = "高崎・前橋", unchecked_value = "" %>高崎・前橋　
			</label>
			<label class="checkbox-inline">
			  <%= f.check_box :city_eq_any, { multiple: true },checked_value = "沼田・みなかみ", unchecked_value = "" %>沼田・みなかみ　
			</label>
			<label class="checkbox-inline">
			  <%= f.check_box :city_eq_any, { multiple: true },checked_value = "草津・渋川・中之条", unchecked_value = "" %>草津・渋川・中之条　
			</label>
			<label class="checkbox-inline">
			 <%= f.check_box :city_eq_any, { multiple: true },checked_value = "藤岡・富岡・安中", unchecked_value = "" %>藤岡・富岡・安中　
			</label>
			
			
	        </div>	    
      	
    
        <div class="pull-right">
        <%= f.submit "検索", class: "btn btn-default" %>
        </div>
        </div>
    </div>

      
    <br>  
<% end %>
</div>
 
</main>

<script>
document.addEventListener("DOMContentLoaded", function (event) {
  navbarToggleSidebar();
  navActivePage();
});
</script>

