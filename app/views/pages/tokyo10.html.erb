<% set_meta_tags noindex: true %>
<%= render "partial/navbar" %>

<main class="" id="main-collapse">

<div class="row">

<div class="hidden-xs col-sm-7 col-md-5">
	<%= render "partial/tokyo" %>
</div>

<div class="col-xs-12 col-sm-5 col-md-7">
	<div class="panel panel-default">
		<%= search_form_for @q, url: search_tokyo10e_path do |f| %>
	    <div class="panel-body">
	      	<p class="form-title">
		      さらに絞り込む
		    </p>
	      		<div class="city-check">
		        <label class="checkbox-inline">
				 <%= f.check_box :station_eq_any, { multiple: true },checked_value = "本郷", unchecked_value = "" %>本郷　
				</label>
				<label class="checkbox-inline">
				  <%= f.check_box :station_eq_any, { multiple: true },checked_value = "水道町", unchecked_value = "" %>水道町　
				</label>
				<label class="checkbox-inline">
				  <%= f.check_box :station_eq_any, { multiple: true },checked_value = "神楽坂", unchecked_value = "" %>神楽坂　
				</label>
				<label class="checkbox-inline">
				  <%= f.check_box :station_eq_any, { multiple: true },checked_value = "飯田橋", unchecked_value = "" %>飯田橋　
				</label>
				<label class="checkbox-inline">
				  <%= f.check_box :station_eq_any, { multiple: true },checked_value = "市ヶ谷", unchecked_value = "" %>市ヶ谷　
				</label>
		        </div>	    
	     
		        <div class="pull-right">
		        <%= f.submit "検索", class: "btn btn-default btn-sm" %>
		        </div>
	        </div>
	    
		<% end %>
	</div>
</div>

</div>
 
</main>

<script>
document.addEventListener("DOMContentLoaded", function (event) {
  navbarToggleSidebar();
  navActivePage();
});
</script>

