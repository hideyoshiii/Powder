<%= render "partial/navbar" %>

<main class="" id="main-collapse">

<div class="panel panel-default">
	
	<%= search_form_for @q, url: search_saitama1_path do |f| %>
    <div class="panel-body">
      	<div class="form-group">
      		<div class="city-check">
	        <label class="checkbox-inline">
			 <%= f.check_box :city_eq_any, { multiple: true },checked_value = "熊谷・深谷・本庄・行田", unchecked_value = "" %>熊谷・深谷・本庄・行田　
			</label>
			<label class="checkbox-inline">
			  <%= f.check_box :city_eq_any, { multiple: true },checked_value = "春日部・越谷・草加", unchecked_value = "" %>春日部・越谷・草加　
			</label>
			<label class="checkbox-inline">
			  <%= f.check_box :city_eq_any, { multiple: true },checked_value = "上尾・久嬉・鴻巣", unchecked_value = "" %>上尾・久嬉・鴻巣　
			</label>
			<label class="checkbox-inline">
			  <%= f.check_box :city_eq_any, { multiple: true },checked_value = "川越・ふじみ野・東松山", unchecked_value = "" %>川越・ふじみ野・東松山　
			</label>
			<label class="checkbox-inline">
			 <%= f.check_box :city_eq_any, { multiple: true },checked_value = "川口・蕨・戸田", unchecked_value = "" %>川口・蕨・戸田　
			</label>
			<label class="checkbox-inline">
			  <%= f.check_box :city_eq_any, { multiple: true },checked_value = "秩父・長瀞・小川", unchecked_value = "" %>秩父・長瀞・小川
			</label>
			<label class="checkbox-inline">
			  <%= f.check_box :city_eq_any, { multiple: true },checked_value = "さいたま市", unchecked_value = "" %>さいたま市　
			</label>
			<label class="checkbox-inline">
			  <%= f.check_box :city_eq_any, { multiple: true },checked_value = "所沢・飯能", unchecked_value = "" %>所沢・飯能　
			</label>
			
	        </div>	    
      	
    
        <div class="pull-right">
        <%= f.submit "検索", class: "btn btn-default" %>
        </div>
        </div>
    </div>

      
    <br>  
<% end %>
</div>
 
</main>

<script>
document.addEventListener("DOMContentLoaded", function (event) {
  navbarToggleSidebar();
  navActivePage();
});
</script>

