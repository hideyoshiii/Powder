<%= render "partial/navbar" %>

<main class="" id="main-collapse">

	<div class="row">

<div class="col-xs-12 col-sm-offset-1 col-sm-10 col-md-offset-2 col-md-8">

<div class="panel panel-default">
	
	<%= search_form_for @q, url: search_hokkaido1_path do |f| %>
    <div class="panel-body">
    	<div class="form-title">
              エリアを絞る
        </div> 
        <hr class="style1">
        
      	<div class="form-group">
      		<div class="city-check">
	        <label class="checkbox-inline">
			 <%= f.check_box :city_eq_any, { multiple: true },checked_value = "旭川・富良野・美瑛", unchecked_value = "" %>旭川・富良野・美瑛　
			</label>
			<label class="checkbox-inline">
			  <%= f.check_box :city_eq_any, { multiple: true },checked_value = "釧路・根室", unchecked_value = "" %>釧路・根室　
			</label>
			<label class="checkbox-inline">
			  <%= f.check_box :city_eq_any, { multiple: true },checked_value = "札幌", unchecked_value = "" %>札幌　
			</label>
			<label class="checkbox-inline">
			  <%= f.check_box :city_eq_any, { multiple: true },checked_value = "小樽・ニセコ・洞爺湖", unchecked_value = "" %>小樽・ニセコ・洞爺湖　
			</label>
			<label class="checkbox-inline">
			  <%= f.check_box :city_eq_any, { multiple: true },checked_value = "夕張・石狩・石見沢", unchecked_value = "" %>夕張・石狩・石見沢　
			</label>
			<label class="checkbox-inline">
			  <%= f.check_box :city_eq_any, { multiple: true },checked_value = "苫小牧・千歳・登別", unchecked_value = "" %>苫小牧・千歳・登別　
			</label>
			<label class="checkbox-inline">
			 <%= f.check_box :city_eq_any, { multiple: true },checked_value = "道南", unchecked_value = "" %>道南　
			</label>
			<label class="checkbox-inline">
			  <%= f.check_box :city_eq_any, { multiple: true },checked_value = "北見・網走・紋別", unchecked_value = "" %>北見・網走・紋別　
			</label>
			<label class="checkbox-inline">
			  <%= f.check_box :city_eq_any, { multiple: true },checked_value = "帯広・十勝", unchecked_value = "" %>帯広・十勝　
			</label>
			
	        </div>	    
      	
    
        <div class="pull-right">
        <%= f.submit "検索", class: "btn btn-default" %>
        </div>
        </div>
    </div>

      
    <br>  
<% end %>
</div>
</div>
</div> 
</main>

<script>
document.addEventListener("DOMContentLoaded", function (event) {
  navbarToggleSidebar();
  navActivePage();
});
</script>

