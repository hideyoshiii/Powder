<% set_meta_tags noindex: true %>
<%= render "partial/navbar" %>

<main class="" id="main-collapse">

<div class="row">

	<div class="col-s-12 col-sm-12 col-md-5">
		<div class="example">
          <%= image_tag "hyogo.jpg", :class =>'img-prefecture' %>
          <p class="prefecture-search1">兵庫</p> 
        </div>
	</div>

	<div class="col-xs-12 col-sm-12 col-md-4">
	<div class="panel panel-default">
		<%= search_form_for @q, url: search_hyogo1_path do |f| %>
	    <div class="panel-body">
	      	<div class="form-title">
              エリアを絞る
        </div> 
        <hr class="style1">
	      		<div class="city-check">
		        <label class="checkbox-inline">
				 <%= f.check_box :station_eq_any, { multiple: true },checked_value = "三田・篠山・西脇", unchecked_value = "" %>三田・篠山・西脇　
				</label>
				<br>
				<label class="checkbox-inline">
				  <%= f.check_box :station_eq_any, { multiple: true },checked_value = "垂水区・西区・北区", unchecked_value = "" %>垂水区・西区・北区　
				</label>
				<br>
				<label class="checkbox-inline">
				  <%= f.check_box :station_eq_any, { multiple: true },checked_value = "西宮・芦屋・尼崎", unchecked_value = "" %>西宮・芦屋・尼崎　
				</label>
				<br>
				<label class="checkbox-inline">
				  <%= f.check_box :station_eq_any, { multiple: true },checked_value = "灘・東灘", unchecked_value = "" %>灘・東灘　
				</label>
				<br>
				<label class="checkbox-inline">
				  <%= f.check_box :station_eq_any, { multiple: true },checked_value = "姫路・加古川・たつの", unchecked_value = "" %>姫路・加古川・たつの　
				</label>
				<br>
				<label class="checkbox-inline">
				  <%= f.check_box :station_eq_any, { multiple: true },checked_value = "兵庫区・長田区・須磨区", unchecked_value = "" %>兵庫区・長田区・須磨区　
				</label>
				<br>
				<label class="checkbox-inline">
				 <%= f.check_box :station_eq_any, { multiple: true },checked_value = "宝塚・伊丹・川西", unchecked_value = "" %>宝塚・伊丹・川西　
				</label>
				<br>
				<label class="checkbox-inline">
				 <%= f.check_box :station_eq_any, { multiple: true },checked_value = "豊岡・朝来", unchecked_value = "" %>豊岡・朝来　
				</label>
				<br>
				<label class="checkbox-inline">
				 <%= f.check_box :station_eq_any, { multiple: true },checked_value = "明石・三木・稲美", unchecked_value = "" %>明石・三木・稲美　
				</label>
				<br>
				<label class="checkbox-inline">
				 <%= f.check_box :station_eq_any, { multiple: true },checked_value = "神戸", unchecked_value = "" %>神戸　
				</label>
				<br>
				<label class="checkbox-inline">
				 <%= f.check_box :station_eq_any, { multiple: true },checked_value = "淡路島", unchecked_value = "" %>淡路島　
				</label>
		        </div>	    
	     
		        <div class="pull-right">
		        <%= f.submit "検索", class: "btn btn-default btn-sm" %>
		        </div>
	        
	    </div> 
		<% end %>
	</div>
</div>

</div>
 
</main>

<script>
document.addEventListener("DOMContentLoaded", function (event) {
  navbarToggleSidebar();
  navActivePage();
});
</script>

