<%= render "partial/navbar" %>

<main class="" id="main-collapse">

	<div class="row">

<div class="col-xs-12 col-sm-offset-1 col-sm-10 col-md-offset-2 col-md-8">

<div class="panel panel-default">
	
	<%= search_form_for @q, url: search_iwate1_path do |f| %>
    <div class="panel-body">
      	<div class="form-group">
      		<div class="city-check">
	        <label class="checkbox-inline">
			 <%= f.check_box :city_eq_any, { multiple: true },checked_value = "奥州・一関", unchecked_value = "" %>奥州・一関　
			</label>
			<label class="checkbox-inline">
			  <%= f.check_box :city_eq_any, { multiple: true },checked_value = "釜石・大船渡", unchecked_value = "" %>釜石・大船渡　
			</label>
			<label class="checkbox-inline">
			  <%= f.check_box :city_eq_any, { multiple: true },checked_value = "久慈・二戸", unchecked_value = "" %>久慈・二戸　
			</label>
			<label class="checkbox-inline">
			  <%= f.check_box :city_eq_any, { multiple: true },checked_value = "宮古・岩泉", unchecked_value = "" %>宮古・岩泉　
			</label>
			<label class="checkbox-inline">
			  <%= f.check_box :city_eq_any, { multiple: true },checked_value = "盛岡・八幡平・雫石", unchecked_value = "" %>盛岡・八幡平・雫石　
			</label>
			<label class="checkbox-inline">
			  <%= f.check_box :city_eq_any, { multiple: true },checked_value = "花巻・北上・遠野", unchecked_value = "" %>花巻・北上・遠野　
			</label>
			
	        </div>	    
      	
    
        <div class="pull-right">
        <%= f.submit "検索", class: "btn btn-default" %>
        </div>
        </div>
    </div>

      
    <br>  
<% end %>
</div>
</div>
</div> 
</main>

<script>
document.addEventListener("DOMContentLoaded", function (event) {
  navbarToggleSidebar();
  navActivePage();
});
</script>

