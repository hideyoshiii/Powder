<%= render "partial/navbar" %>

<main class="" id="main-collapse">

<div class="row">

<div class="hidden-xs col-sm-7 col-md-5">
	<%= render "partial/tokyo" %>
</div>

<div class="col-xs-12 col-sm-5 col-md-7">
	<div class="panel panel-default">
		<%= search_form_for @q, url: search_tokyo23e_path do |f| %>
	    <div class="panel-body">
	      	<div class="form-group">
	      		<div class="city-check">
		        <label class="checkbox-inline">
				 <%= f.check_box :station_eq_any, { multiple: true },checked_value = "高尾", unchecked_value = "" %>高尾　
				</label>
				<label class="checkbox-inline">
				  <%= f.check_box :station_eq_any, { multiple: true },checked_value = "八王子", unchecked_value = "" %>八王子　
				</label>
				<label class="checkbox-inline">
				  <%= f.check_box :station_eq_any, { multiple: true },checked_value = "福生", unchecked_value = "" %>福生　
				</label>
				<label class="checkbox-inline">
				  <%= f.check_box :station_eq_any, { multiple: true },checked_value = "町田", unchecked_value = "" %>町田島　
				</label>
				<label class="checkbox-inline">
				  <%= f.check_box :station_eq_any, { multiple: true },checked_value = "立川", unchecked_value = "" %>立川　
				</label>
				<label class="checkbox-inline">
				  <%= f.check_box :station_eq_any, { multiple: true },checked_value = "日野・昭島", unchecked_value = "" %>日野・昭島　
				</label>
				<label class="checkbox-inline">
				 <%= f.check_box :station_eq_any, { multiple: true },checked_value = "奥多摩", unchecked_value = "" %>奥多摩　
				</label>
				<label class="checkbox-inline">
				 <%= f.check_box :station_eq_any, { multiple: true },checked_value = "青梅", unchecked_value = "" %>青梅　
				</label>
		        </div>	    
	     
		        <div class="pull-right">
		        <%= f.submit "検索", class: "btn btn-default btn-sm" %>
		        </div>
	        </div>
	    </div> 
		<% end %>
	</div>
</div>

</div>
 
</main>

<script>
document.addEventListener("DOMContentLoaded", function (event) {
  navbarToggleSidebar();
  navActivePage();
});
</script>

