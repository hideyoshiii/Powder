<%= render "partial/navbar" %>

<main class="" id="main-collapse">

<div class="row">

<div class="col-xs-12 col-sm-offset-1 col-sm-10 col-md-offset-2 col-md-8">
	<div class="panel panel-default">
		<%= search_form_for @q, url: search_kagawa1_path do |f| %>
	    <div class="panel-body">
	      	<p class="form-title">
		      さらに絞り込む
		    </p>
	      		<div class="city-check">
		        <label class="checkbox-inline">
				 <%= f.check_box :station_eq_any, { multiple: true },checked_value = "丸亀・坂出・綾川", unchecked_value = "" %>丸亀・坂出・綾川　
				</label>
				<label class="checkbox-inline">
				  <%= f.check_box :station_eq_any, { multiple: true },checked_value = "高松・三木", unchecked_value = "" %>高松・三木　
				</label>
				<label class="checkbox-inline">
				  <%= f.check_box :station_eq_any, { multiple: true },checked_value = "三豊・観音寺", unchecked_value = "" %>三豊・観音寺　
				</label>
				<label class="checkbox-inline">
				  <%= f.check_box :station_eq_any, { multiple: true },checked_value = "小豆島・直島", unchecked_value = "" %>小豆島・直島　
				</label>
				<label class="checkbox-inline">
				  <%= f.check_box :station_eq_any, { multiple: true },checked_value = "讃岐・東かがわ", unchecked_value = "" %>讃岐・東かがわ　
				</label>
				
		        </div>	    
	     
		        <div class="pull-right">
		        <%= f.submit "検索", class: "btn btn-default btn-sm" %>
		        </div>
	        
	    </div> 
		<% end %>
	</div>
</div>

</div>
 
</main>

<script>
document.addEventListener("DOMContentLoaded", function (event) {
  navbarToggleSidebar();
  navActivePage();
});
</script>

