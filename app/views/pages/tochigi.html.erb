<%= render "partial/navbar" %>

<main class="" id="main-collapse">

<div class="panel panel-default">
	
	<%= search_form_for @q, url: search_tochigi1_path do |f| %>
    <div class="panel-body">
      	<div class="form-group">
      		<div class="city-check">
	        <label class="checkbox-inline">
			 <%= f.check_box :city_eq_any, { multiple: true },checked_value = "宇都宮・鹿沼・下野", unchecked_value = "" %>宇都宮・鹿沼・下野　
			</label>
			<label class="checkbox-inline">
			  <%= f.check_box :city_eq_any, { multiple: true },checked_value = "太田原・那須塩原", unchecked_value = "" %>太田原・那須塩原　
			</label>
			<label class="checkbox-inline">
			  <%= f.check_box :city_eq_any, { multiple: true },checked_value = "栃木・小山・佐野・足利", unchecked_value = "" %>栃木・小山・佐野・足利　
			</label>
			<label class="checkbox-inline">
			  <%= f.check_box :city_eq_any, { multiple: true },checked_value = "那珂川・真岡・益子", unchecked_value = "" %>那珂川・真岡・益子　
			</label>
			<label class="checkbox-inline">
			 <%= f.check_box :city_eq_any, { multiple: true },checked_value = "日光", unchecked_value = "" %>日光　
			</label>
			<label class="checkbox-inline">
			  <%= f.check_box :city_eq_any, { multiple: true },checked_value = "矢板・さくら・塩谷", unchecked_value = "" %>矢板・さくら・塩谷
			</label>
			
	        </div>	    
      	
    
        <div class="pull-right">
        <%= f.submit "検索", class: "btn btn-default" %>
        </div>
        </div>
    </div>

      
    <br>  
<% end %>
</div>
 
</main>

<script>
document.addEventListener("DOMContentLoaded", function (event) {
  navbarToggleSidebar();
  navActivePage();
});
</script>

