<%= render "partial/navbar" %>
<main class="" id="main-collapse">
<br>

  <span style="color:black; font-size:18px;border-bottom: solid 2px black;">new</span>
  &nbsp;&nbsp;
  <%= link_to spots_rank_path do %>
  <span class="black" style="font-size:18px;">rank</span>
  <% end %>
  <br><br>

  <div class="hero-full-wrapper">
  <div class="grid">
  <div class="gutter-sizer"></div>
    <div class="grid-sizer"></div>
    <% @spots.each do |spot| %>
    <div class="grid-item iii">

      <%= link_to spot_path(spot) do %>
      <%= image_tag spot.photo.url, :class =>'img-responsive' %>
      <h4 class="black img-show title-font-index"><%= spot.title %></h4>
      <% end %>

      <div class="img-show prefecture-font-index">
        <span style="color: #CCCCCC;"><i class="fa fa-map-marker" aria-hidden="true"></i><%= spot.prefecture %></span>
      </div>

      <span id="like-of-<%= spot.id %>">
        <%= render partial: 'likes/like', locals: { spot: spot, likes: @likes } %>
      </span>
      
      <br>
      
    </div>
    <% end %>  
  </div>
</div>


<script>
  document.addEventListener("DOMContentLoaded", function (event) {
     masonryBuild();
  });
</script>
 
</main>

<script>
document.addEventListener("DOMContentLoaded", function (event) {
  navbarToggleSidebar();
  navActivePage();
});
</script>

