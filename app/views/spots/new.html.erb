<%= render "partial/navbar" %>
<main class="" id="main-collapse">

<div class="container">
	<div class="row">
		<br>
		<div class="col-xs-12 col-sm-8 col-sm-offset-2 col-md-6 col-md-offset-3">
			<div class="panel panel-default">
				<div class="panel-body">		
					<div class="title-content">
					  デートスポットを投稿する
					</div> 
					<hr class="style1">	
					
					<%= form_for @spot, :html => { multipart: true } do |f| %>

					<div class="row">
		            	<div class="col-md-12 select">
		              		<div class="form-group">
		                		<label>photo</label>
		            			<div class="field">
		              				<%= f.file_field :image %>
		            			</div>
		            		</div>
		            	</div>
		            </div>
		            <br>

          
						<div class="row">
							<div class="col-md-12 select">
								<div class="form-group">
                                <label>title</label>
                                <%= f.text_field :title, placeholder: "スポットの名称", class: "form-control", required: "true" %>
                            	</div>
							</div>
						</div>
						<br>

						<div class="row">
							<div class="col-md-12 select">
								<div class="form-group">
									<label>category</label>
									<%= f.select :large, [["レストラン","レストラン"], ["バー","バー"], ["カフェ","カフェ"], ["アクティブ","アクティブ"], ["夜景","夜景"], ["その他","その他"]], {prompt:"選択してください"}, {required: 'true', autofocus: 'true', class: 'form-control'} %>
								</div>
							</div>
						</div>
						<br>

						<div class="row">
							<div class="col-md-12 select">
								<div class="form-group">
									<label>prefecture</label>
									<%= f.select :prefecture, [["北海道","北海道"], ["青森","青森"], ["岩手","岩手"], ["宮城","宮城"], ["秋田","秋田"], ["山形","山形"], ["福島","福島"], ["茨城","茨城"], ["栃木","栃木"], ["群馬","群馬"], ["埼玉","埼玉"], ["千葉","千葉"], ["東京","東京"], ["神奈川","神奈川"], ["新潟","新潟"], ["富山","富山"], ["石川","石川"], ["福井","福井"], ["山梨","山梨"], ["長野","長野"], ["岐阜","岐阜"], ["静岡","静岡"], ["愛知","愛知"], ["三重","三重"], ["滋賀","滋賀"], ["京都","京都"], ["大阪","大阪"], ["兵庫","兵庫"], ["奈良","奈良"], ["和歌山","和歌山"], ["鳥取","鳥取"], ["島根","島根"], ["岡山","岡山"], ["広島","広島"], ["山口","山口"], ["徳島","徳島"], ["香川","香川"], ["愛媛","愛媛"], ["高知","高知"], ["福岡","福岡"], ["佐賀","佐賀"], ["長崎","長崎"], ["熊本","熊本"], ["大分","大分"], ["宮崎","宮崎"],["鹿児島","鹿児島"],["沖縄","沖縄"],["海外","海外"], ["家","家"]], {prompt:"選択してください"}, {required: 'true', autofocus: 'true', class: 'form-control'} %>
								</div>
							</div>
						</div>
						<br>

						<div class="row">
							<div class="col-md-12 select">
								<div class="form-group">
                                <label>city</label>
                                <%= f.text_field :city, placeholder: "エリア", class: "form-control", required: "true" %>
                            	</div>
							</div>
						</div>
						<br>

						<div class="row">
							<div class="col-md-12 select">
								<div class="form-group">
                                <label>statiopn</label>
                                <%= f.text_field :station, placeholder: "最寄駅", class: "form-control", required: "true" %>
                            	</div>
							</div>
						</div>
						<br>	

			            <div class="row">
			              <div class="col-md-12 select">
			                <div class="field">
			                  <%= f.label :label_list, "tag" %><br>
			                  <%= text_field_tag 'spot[label_list]', @spot.label_list.join(','), "data-role" => "tagsinput" %>
			                </div>
			              </div>
			            </div>
			            <br>

						<div class="row">
							<div class="col-md-12 select">
								<div class="form-group">
                                <label>description</label>
                                <%= f.text_area :description, rows: 7, placeholder: "説明", class: "form-control" %>
                            </div> 
							</div>
						</div>
						<br>

						<div class="row">
							<div class="col-md-12 select">
								<div class="form-group">
                                <label>address</label>
                                <%= f.text_field :address, placeholder: "住所", class: "form-control" %>
                            	</div>
							</div>
						</div>
						<br>

						<div class="row">
							<div class="col-md-12 select">
								<div class="form-group">
                                <label>instagram</label>
                                <%= f.text_field :introduction, placeholder: "インスタグラムのURL", class: "form-control" %>
                            	</div>
							</div>
						</div>
						<br>

						<div class="row">
							<div class="col-md-12 select">
								<div class="form-group">
                                <label>taberogu</label>
                                <%= f.text_field :retty, placeholder: "食べログのURL", class: "form-control" %>
                            	</div>
							</div>
						</div>
						<br>

						<div class="row">
							<div class="col-md-12 select">
								<div class="form-group">
                                <label>browser</label>
                                <%= f.text_field :browser, placeholder: "公式サイトのURL", class: "form-control" %>
                            	</div>
							</div>
						</div>
						<br>

						<div class="row">
							<div class="col-md-12 select">
								<div class="form-group">
                                <label>phone</label>
                                <%= f.text_field :phone, placeholder: "電話番号(半角ハイフンなし)", class: "form-control" %>
                            	</div>
							</div>
						</div>
						<br>



						

						<div class="actions">
							<%= f.submit "保存", class: "btn btn-primary pull-right" %>
						</div>	
					<% end %>
				</div>
			</div>
		</div>
	</div>
</div>


 
</main>

<script>
document.addEventListener("DOMContentLoaded", function (event) {
  navbarToggleSidebar();
  navActivePage();
});
</script>

