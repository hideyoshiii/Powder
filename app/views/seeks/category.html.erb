            水&nbsp;&nbsp;<% unless @spot1.wednesday.blank? %><%= @spot1.wednesday %><% end %><br>
            木&nbsp;&nbsp;<% unless @spot1.thursday.blank? %><%= @spot1.thursday %><% end %><br>
            金&nbsp;&nbsp;<% unless @spot1.friday.blank? %><%= @spot1.friday %><% end %><br>
            土&nbsp;&nbsp;<% unless @spot1.saturday.blank? %><%= @spot1.saturday %><% end %><br>
            日&nbsp;&nbsp;<% unless @spot1.sunday.blank? %><%= @spot1.sunday %><% end %><br>
            祝&nbsp;&nbsp;<% unless @spot1.holiday.blank? %><%= @spot1.holiday %><% end %><br>
          </p> 
        </div>
        <%# 見出し %>
        <% unless @spot1.heading.blank? %>
          <p class="aaa-min"><%= @spot1.heading %></p>
        <% end %>
