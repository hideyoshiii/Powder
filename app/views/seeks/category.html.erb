          <% end %>
          <% unless @spot1.introduction.blank? %>
            <a href="<%= @spot1.introduction %>" target="_blank" class="raise4-result"><span><i class="fa fa-instagram"></i>&nbsp;Instagram</span></a>
          <% end %>
        </div>
        <button type="button" class="btn btn-default pull-right" data-dismiss="modal">×&nbsp;閉じる</button>
      </div>  
